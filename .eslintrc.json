{
    "extends": "airbnb",
    "plugins": [
        "react",
        "import",
        "jsx-a11y"
    ],
    "rules": {
      "no-use-before-define": 0,
      "react/jsx-filename-extension": 0,
      "prefer-stateless-function": 0,
      "react/prop-types": 0,
      "react/jsx-no-bind": 0
    },
    "parser": "babel-eslint",
    "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
    }
}
